.empty_tip{
  position: fixed;
  height: 100%;
  width: 100%;
  display:flex;
  flex-direction:column;
  align-items:center;/*垂直居中*/
  justify-content: center;/*水平居中*/
}

.container_title{
  width: 750rpx;
  height: 250rpx;
  background-color: RGB(68,140,199);
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-evenly;
}


